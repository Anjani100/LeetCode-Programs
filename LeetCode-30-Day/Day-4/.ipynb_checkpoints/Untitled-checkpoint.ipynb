   "source": [
    "nums = [int(i) for i in input().split()]\n",
    "\n",
    "count = 0\n",
    "i = 0\n",
    "while (i < len(nums)):\n",
    "    if(nums[i] == 0):\n",
    "        nums.pop(i)\n",
    "        i -= 1\n",
    "        count += 1\n",
    "    i += 1\n",
    "while(count > 0):\n",
    "    nums.append(0)\n",
    "    count -= 1"
   ]
  }
